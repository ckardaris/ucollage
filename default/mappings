[]
type    builtin
map     ++
script  _set --- gridlines+=1 gridcolumns+=1
command set

[]
type    builtin
map     --
script  _set --- gridlines-=1 gridcolumns-=1
command set

[]
type    builtin
map     +|
script  _set --- gridcolumns+=1
command set

[]
type    builtin
map     -|
script  _set --- gridcolumns-=1
command set

[]
type    builtin
map     +_
